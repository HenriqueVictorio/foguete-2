{
  "version": 2,
  "builds": [
    {
      "src": "client/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ],
  "env": {
    "SOCKET_URL": "https://crash-rocket-server.railway.app"
  },
  "functions": {
    "client/**/*": {
      "includeFiles": "client/**"
    }
  }
}
